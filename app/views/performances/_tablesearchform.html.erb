<div class="row">
	<div class="col-md-9">
		<div class="card card-sheet sheet-condensed">
			<table class="table table-striped table-hover table-responsive-sm">
				<thead>
					<tr>
							<th>Branch</th>
							<th>Defaulted(times)</th>
							<th></th>
					</tr>
				</thead>
				<tbody>
				<% @final_results.each do |fres, def_time| %>
					<tr>
							<td><%= fres %></td>
							<td><%= def_time %></td>
							<td class="actions text-right">
							<div class="btn-group">
								<%#= link_to "View", "#", class: "btn btn-danger" %>

							</div>
							</td>
						</tr>
						<% end %>
						<% if @final_results.length == 0 %>
						<tr><td colspan=42 class='text-center'>No questionnaires to query yet.</td></tr>
						<% end %>
						</tbody>
					</table>
		</div>
	</div>
 </div>

