<div class="row">
          <div class="col-md-12">

            <div class="card card-sheet sheet-condensed">
              <table class="table table-striped table-hover table-sm-responsive">
                <thead>
                  <tr>
                                          <th>First name</th>
                                          <th>Last name</th>
                                          <th>Branch</th>
                                          <th>Council</th>
                                          <th>Whatsapp number</th>
                                          <th>Year treasurer</th>
                                          <th>Treasurer type</th>
                                        <th></th>
                  </tr>
                </thead>
                <tbody>
                <% @treasurers.each do |treasurer| %>
                  <tr>
										<td><%= treasurer.first_name %></td>
										<td><%= treasurer.last_name %></td>
										<td><%= treasurer.branch_id %></td>
										<td><%= treasurer.council %></td>
										<td><%= treasurer.whatsapp_number %></td>
										<td><%= treasurer.year_treasurer %></td>
										<td><%= treasurer.treasurer_type %></td>
									<td class="actions text-right">
                      <div class="btn-group">
                        <%= link_to theme_icon_tag("eye"), treasurer, 'data-toggle': 'tooltip', title: 'Show', class: "btn btn-link" %>
                        <%= link_to theme_icon_tag("pencil-alt"), edit_treasurer_path(treasurer), 'data-toggle': 'tooltip', title: 'Edit', class: "btn btn-link" %>
                        <%= link_to theme_icon_tag("trash"), treasurer, 'data-toggle': 'tooltip', title: 'Delete',
                        data: { confirm: 'Are you sure?' }, method: :delete, class: 'btn btn-link' %>
                      </div>
                      </td>
                    </tr>
                    <% end %>
                    <% if @treasurers.length == 0 %>
                    <tr><td colspan=42 class='text-center'>No treasurers.</td></tr>
                    <% end %>
                    </tbody>
                  </table>
                </div>

                                  <%# Remove the trailing if condition if you're using pagination. %>
                  <%= will_paginate @treasurers if @treasurers.respond_to?(:total_pages) %>
                
                              </div>
			</div>
          